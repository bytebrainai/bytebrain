{
  "name": "web-components",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "concurrently \"yarn:watch:*\"",
    "build-storybook": "concurrently \"yarn:build:*\"",
    "watch:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "watch:css": "npx tailwindcss -i ./src/tailwind.css -o ./src/styles/tailwind.css --watch",
    "build:css": "npx tailwindcss -i ./src/tailwind.css -o ./src/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.0.6",
    "@storybook/blocks": "^7.0.8",
    "@storybook/preset-create-react-app": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-webpack5": "^7.0.8",
    "@storybook/testing-library": "^0.0.14-next.2",
    "autoprefixer": "^10.4.14",
    "babel-plugin-named-exports-order": "^0.0.2",
    "postcss": "^8.4.23",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.8",
    "tailwindcss": "^3.3.2",
    "webpack": "^5.81.0",
    "concurrently": "^7.6.0"
  }
}
